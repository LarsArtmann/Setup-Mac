Host github.com
        UseKeychain no
        ControlMaster auto
        ControlPath ~/.ssh/sockets/%r@%h-%p
        ControlPersist 600
        ServerAliveInterval 60
        User git
        Compression yes
        TCPKeepAlive yes
        IPQoS throughput
