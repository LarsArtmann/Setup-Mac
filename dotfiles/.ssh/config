# Added by OrbStack: 'orb' SSH host for Linux machines
# This only works if it's at the top of ssh_config (before any Host blocks).
# This won't be added again if you remove it.
Include ~/.orbstack/ssh/config

Include /Users/larsartmann/.colima/ssh_config

##Host *
##	IdentityAgent /Users/larsartmann/Library/Containers/com.maxgoedjen.Secretive.SecretAgent/Data/socket.ssh

Host private-cloud-hetzner-0
        HostName 37.27.217.205
        User root
        PreferredAuthentications publickey

Host private-cloud-hetzner-1
        HostName 37.27.195.171
        User root
        PreferredAuthentications publickey

Host private-cloud-hetzner-2
        HostName 37.27.24.111
        User root
        PreferredAuthentications publickey

Host github.com
        UseKeychain no
        ControlMaster auto
        ControlPath ~/.ssh/sockets/%r@%h-%p
        ControlPersist 600
        ServerAliveInterval 60
        User git
        Compression yes
        TCPKeepAlive yes
        IPQoS throughput

# --- START CODER JETBRAINS a9earag3ksvfq.pit-1.try.coder.app
Host coder-jetbrains-a9earag3ksvfq.pit-1.try.coder.app--*
  ProxyCommand "/Users/larsartmann/Library/Application Support/coder-gateway/a9earag3ksvfq.pit-1.try.coder.app/coder-darwin-arm64" --global-config "/Users/larsartmann/Library/Application Support/coder-gateway/a9earag3ksvfq.pit-1.try.coder.app/config" --url https://a9earag3ksvfq.pit-1.try.coder.app/ ssh --stdio --disable-autostart --usage-app=jetbrains --ssh-host-prefix coder-jetbrains-a9earag3ksvfq.pit-1.try.coder.app-- %h
  ConnectTimeout 0
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  LogLevel ERROR
  SetEnv CODER_SSH_SESSION_TYPE=JetBrains

Host coder-jetbrains-a9earag3ksvfq.pit-1.try.coder.app-bg--*
  ProxyCommand "/Users/larsartmann/Library/Application Support/coder-gateway/a9earag3ksvfq.pit-1.try.coder.app/coder-darwin-arm64" --global-config "/Users/larsartmann/Library/Application Support/coder-gateway/a9earag3ksvfq.pit-1.try.coder.app/config" --url https://a9earag3ksvfq.pit-1.try.coder.app/ ssh --stdio --disable-autostart --usage-app=disable --ssh-host-prefix coder-jetbrains-a9earag3ksvfq.pit-1.try.coder.app-bg-- %h
  ConnectTimeout 0
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  LogLevel ERROR
  SetEnv CODER_SSH_SESSION_TYPE=JetBrains
# --- END CODER JETBRAINS a9earag3ksvfq.pit-1.try.coder.app
