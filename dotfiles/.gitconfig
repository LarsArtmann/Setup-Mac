[user]
	name = Lars Artmann
	email = git@lars.software
	signingkey = 76687BB69B36BFB1B1C58FA878B4350389C71333
[core]
	autocrlf = input
	compression = 9
	packedGitLimit = 512m
	packedGitWindowSize = 512m
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[url "ssh://git@github.com/LarsArtmann"]
	insteadOf = https://github.com/LarsArtmann
[url "ssh://git@github.com/creditornot"]
	insteadOf = https://github.com/creditornot
[commit]
	gpgsign = true
[tag]
	gpgsign = true
[submodule]
	fetchJobs = 8
[http]
	postBuffer = 524288000
[ssh]
	multiplexing = true
